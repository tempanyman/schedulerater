<p id="notice"><%= notice %></p>

<h1>Class Reviews</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @class_reviews.each do |class_review| %>
      <tr>
        <td><%= link_to 'Show', class_review %></td>
        <td><%= link_to 'Edit', edit_class_review_path(class_review) %></td>
        <td><%= link_to 'Destroy', class_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Class Review', new_class_review_path %>
